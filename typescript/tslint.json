{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-consecutive-blank-lines": false, 
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
          ]
    },
    "rulesDirectory": []
}